	<RecipeDef ParentName="MakeBulkStoneBlocksBaseNoElecAny">
		<defName>MakeStoneBlocksAny5</defName>
		<label>make any stone blocks (5)</label>
		<description>Cuts rock chunks into usable stone blocks in bulk.</description>
		<workerCounterClass>RecipeWorkerCounter_MakeStoneBlocks</workerCounterClass>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>StoneChunks</li>
					</categories>
				</filter>
				<count>5</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>StoneChunks</li>
			</categories>
		</fixedIngredientFilter>
