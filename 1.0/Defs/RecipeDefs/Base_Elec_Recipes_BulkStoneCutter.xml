<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!--Elec Bulk Specialized 5-->
	<RecipeDef ParentName="RF_BSC_MakeBulkStoneBlocksBaseElec">
		<defName>MakeSlateBlocks5E</defName>
		<label>make slate blocks (5)</label>
		<description>Cuts slate chunks into usable slate blocks in bulk.</description>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>ChunkSlate</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
					<thingDefs>
						<li>ChunkSlate</li>
					</thingDefs>
		</fixedIngredientFilter>
        <products>
             <BlocksSlate>100</BlocksSlate>
        </products>
	</RecipeDef>
